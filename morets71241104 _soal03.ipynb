      "source": [
        "import math\n",
        "\n",
        "P = 200000000\n",
        "A = 400000000\n",
        "r = 0.10\n",
        "\n",
        "t = math.log(A / P) / math.log(1 + r)\n",
        "\n",
        "print(\"Minimal waktu untuk mencapai 400 juta:\", round(t, 2), \"tahun\")\n"
      ]
    }
